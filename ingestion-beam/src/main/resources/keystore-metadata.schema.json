{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "metadata for the key store",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "document_namespace": {
                "description": "The identifier of the study.",
                "type": "string"
            },
            "kms_resource_id": {
                "description": "The fully qualified keyring resource name in KMS.",
                "type": "string"
            },
            "private_key_uri": {
                "description": "Path of the key in the local filesystem or GCS.",
                "type": "string"
            }
        },
        "required": ["document_namespace", "kms_resource_id", "private_key_uri"]
    }
}